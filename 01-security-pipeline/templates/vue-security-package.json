{
  "name": "vue-security-template",
  "version": "1.0.0",
  "description": "Vue.js项目安全配置模板",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:security": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --config .eslintrc.security.js",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "security:check": "bash ../scripts/vue-security-check.sh",
    "security:deps": "npm audit && retire",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.3.0",
    "vue-router": "^4.2.0",
    "pinia": "^2.1.0",
    "axios": "^1.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.3.0",
    "@vue/tsconfig": "^0.4.0",
    "@rushstack/eslint-patch": "^1.3.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.49.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "retire": "^4.0.0",
    "typescript": "~5.2.0",
    "vite": "^4.4.0",
    "vue-tsc": "^1.8.0"
  }
}